{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "clean": "rimraf public",
    "develop": "gatsby develop",
    "deploy": "gatsby build --prefix-paths && gatsby-plugin-s3 deploy -y",
    "start": "npm run develop",
    "format": "prettier --write \"{,!(node_modules|public|coverage|.cache)/**/}*.{js,jsx,ts,tsx,json}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "gatsby serve --prefix-paths -o",
    "build-no-prefix": "gatsby build",
    "serve-no-prefix": "gatsby serve"
  },
  "dependencies": {
    "gatsby": "^2.17.11",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "gatsby-plugin-s3": "^0.3.2",
    "typescript": "^3.2.2",
    "gatsby-plugin-typescript": "^2.0.3"
  },
  "devDependencies": {
    "prettier": "^1.19.1",
    "react-bootstrap": "^1.0.0-beta.14",
    "bootstrap": "^4.3.1"
  }
}
